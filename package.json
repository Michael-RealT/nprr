{
  "name": "npr",
  "version": "0.0.0-development",
  "description": "npm run + autocomplete ðŸŽ‰",
  "main": "src/index.js",
  "bin": {
    "npr": "./src/bin.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "unit:test": "jest",
    "unit:test:watch": "jest --watch",
    "semantic-release": "semantic-release",
    "commit": "git-cz",
    "// INSTALLATION /////////////////////": "",
    "global-installation": "npm pack | pbcopy && npm i -g $(pbpaste)",
    "global-uninstallation": "npm uninstall -g npr",
    "// TESTS ////////////////////////////": "",
    "test": "npm run e2e:test:global-installation",
    "test:global-installation": "cd test-project/local-installation && npr test && cd ../..",
    "e2e:test:global-installation": "npm run global-installation && npm run test:global-installation && npm run global-uninstallation"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NoriSte/npr.git"
  },
  "keywords": [
    "npm run",
    "autocomplete"
  ],
  "author": "NoriSte <nori.ste.magni@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NoriSte/npr/issues"
  },
  "homepage": "https://github.com/NoriSte/npr#readme",
  "devDependencies": {
    "cz-conventional-changelog": "^3.0.2",
    "jest": "^24.9.0",
    "semantic-release": "^15.13.24",
    "wait-for-expect": "^3.0.0"
  },
  "dependencies": {
    "enquirer": "^2.3.2",
    "npm": "^6.12.0"
  }
}
