{
  "name": "npr",
  "version": "0.0.0-development",
  "description": "npm run + autocomplete ðŸŽ‰",
  "main": "src/index.js",
  "bin": {
    "npr": "./src/bin.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "unit:test": "jest",
    "unit:test:coverage": "jest --collectCoverage",
    "unit:test:watch": "jest --watch",
    "semantic-release": "semantic-release",
    "commit": "git-cz",
    "install-on-your-machine": "npm pack && sudo npm i -g $(find . -name 'npr-*') && sudo -k",
    "// TESTS ////////////////////////////": "",
    "test": "npm run unit:test && npm run test:import-npr",
    "test:import-npr": "npm pack && cd test-projects/import-npr && npm i $(find ../../ -name 'npr-*') && npm run script && cd ../..",
    "test:trap": "jest -t 'Trap succeed'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NoriSte/npr.git"
  },
  "keywords": [
    "npm run",
    "autocomplete"
  ],
  "author": "NoriSte <nori.ste.magni@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NoriSte/npr/issues"
  },
  "homepage": "https://github.com/NoriSte/npr#readme",
  "devDependencies": {
    "coveralls": "^3.0.7",
    "cz-conventional-changelog": "^3.0.2",
    "jest": "^24.9.0",
    "semantic-release": "^15.13.24"
  },
  "dependencies": {
    "enquirer": "^2.3.2",
    "npm": "^6.12.0"
  }
}
